{
    "name": "cristake/l5-newsletter",
    "description": "Laravel 5.1.x package to use with Mailchimp API v3.0",
    "type": "package",
    "keywords": ["laravel", "newsletter"],
    "license": "MIT",
    "authors": [
        {
            "name": "Cristian Iosif",
            "email": "cristianiosif@me.com"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "illuminate/support": "~5.0",
        "pacely/mailchimp-apiv3": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Cristake\\Newsletter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cristake\\Newsletter\\Test\\MailChimp\\": "tests"
        }
    },
    "minimum-stability": "dev"
}
