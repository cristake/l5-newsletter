            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "illuminate/support": "~5.0",
        "pacely/mailchimp-apiv3": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
